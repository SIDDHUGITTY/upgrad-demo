pipeline {
    agent any
    stages {
        stage('checkout SCM') {
            steps {
                echo 'checkout SCM stage'
            }
        }
        stage('build the code') {
            steps {
                echo 'build the code'
            }
        }
        stage('test the code') {
            steps {
                echo 'test the code'
            }    
        }    
        stage('deploy the code') {
            steps {
                echo 'deploy the code'    
            }
        }
    }    
}
